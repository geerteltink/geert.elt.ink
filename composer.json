{
    "name": "xtreamwayz/xtreamwayz.com",
    "description": "Source code behind xtreamwayz.com",
    "type": "project",
    "homepage": "https://github.com/xtreamwayz/xtreamwayz.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Geert Eltink",
            "homepage": "https://xtreamwayz.com/",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "url": "https://github.com/xtreamwayz/psr7-sessions-storageless",
            "type": "vcs"
        }
    ],
    "require": {
        "php": "^7.0",
        "dflydev/fig-cookies": "^1.0",
        "doctrine/cache": "^1.6",
        "guzzlehttp/guzzle": "^6.2",
        "michelf/php-markdown": "^1.7",
        "monolog/monolog": "^1.22",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0",
        "psr7-sessions/storageless": "^4.0 || dev-feature/psr-http-middleware",
        "roave/security-advisories": "dev-master",
        "symfony/yaml": "^3.2",
        "xtreamwayz/html-form-validator": "^1.0 || 1.0.x-dev",
        "zendframework/zend-config-aggregator": "^1.0",
        "zendframework/zend-expressive": "~2.0.0",
        "zendframework/zend-expressive-fastroute": "^2.1",
        "zendframework/zend-expressive-helpers": "^4.0",
        "zendframework/zend-expressive-twigrenderer": "^1.3",
        "zendframework/zend-feed": "^2.6",
        "zendframework/zend-inputfilter": "^2.7",
        "zendframework/zend-mail": "^2.7",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stdlib": "^3.1",
        "zendframework/zend-stratigility": "~2.0.1"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "phpstan/phpstan": "^0.8.2",
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0",
        "zendframework/zend-component-installer": "^1.0",
        "zendframework/zend-expressive-tooling": "^1.0 || ^0.4",
        "zfcampus/zf-development-mode": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "check": [
            "@cs-check",
            "@php-stan",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "php-stan": "phpstan analyse -l 5 src test",
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    }
}
