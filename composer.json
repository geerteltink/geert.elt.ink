{
    "name": "xtreamwayz/xtreamwayz.com",
    "description": "Source code behind xtreamwayz.com",
    "type": "project",
    "homepage": "https://github.com/xtreamwayz/xtreamwayz.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Geert Eltink",
            "homepage": "https://xtreamwayz.com/"
        }
    ],
    "require": {
        "php": "^7.0",
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "^1.0",
        "zendframework/zend-expressive-helpers": "^2.0",
        "zendframework/zend-expressive-fastroute": "^1.0",
        "zendframework/zend-expressive-twigrenderer": "^1.1",
        "zendframework/zend-stdlib": "^3.0",
        "zendframework/zend-servicemanager": "^3.0",
        "zendframework/zend-feed": "^2.6",
        "zendframework/zend-inputfilter": "^2.5",
        "zendframework/zend-mail": "^2.6",
        "ocramius/psr7-session": "^1.0",
        "guzzlehttp/guzzle": "^6.1",
        "michelf/php-markdown": "^1.6",
        "symfony/yaml": "^3.0",
        "doctrine/cache": "^1.6",
        "monolog/monolog": "^1.17",
        "xtreamwayz/html-form-validator": "1.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "squizlabs/php_codesniffer": "^2.3",
        "filp/whoops": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
